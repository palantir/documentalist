{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to debugger",
            "type": "node",
            "request": "attach",
            "port": 9229
        },
        {
            "name": "Documentalist",
            "type": "node",
            "request": "launch",
            "args": ["src/launch.ts"],
            "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector"
        }
    ],
    "compounds": []
}
